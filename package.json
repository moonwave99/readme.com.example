{
  "name": "readme.com-example",
  "version": "0.0.1",
  "description": "readme.com example",
  "main": "index.js",
  "type": "module",
  "readme.com": {
    "syntaxHighlight": false
  },
  "scripts": {
    "build": "rm -rf dist && npx readme.com --templatePath templates",
    "dev": "npx concurrently 'npx serve dist' 'npx nodemon --watch readme.md --exec npm run build'"
  }
}
